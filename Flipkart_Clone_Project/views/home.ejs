<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Flipkart</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/style.css">
     <link
    href="https://cdn.jsdelivr.net/npm/remixicon@4.2.0/fonts/remixicon.css"
    rel="stylesheet"
/>

</head>
<body>
    <div id="main-text">
      <div id="navbar-start">
        <div class="nav-item-up">
          <div class="logo"><a href="/home"><img src="https://static-assets-web.flixcart.com/batman-returns/batman-returns/p/images/fkheaderlogo_exploreplus-44005d.svg" alt=""></a></div>
          <i class="ri-search-line" id="icon"></i><input type="text" class="search" placeholder="Search for Products,Brands and More">
          <i class="ri-account-circle-line" id="account"></i><a href="/userlogin" style="text-decoration: none;color: black;"><h3 class="item">Login</h3></a>
          <i class="ri-shopping-cart-line" id="cartes"></i><a href="/cart" style="text-decoration: none;color: black;"><h3 class="item"  >Cart</h3></a>
          <i class="ri-store-line" id="store"></i><a href="/registration" style="text-decoration: none;color: black;"><h3 class="item">Register</h3></a>
          
        </div>
      </div>
      <div id="box2">
        <div class="boxs-item">
          <img src="https://rukminim2.flixcart.com/flap/64/64/image/29327f40e9c4d26b.png?q=100" alt="">
          <h3>Grocery</h3>
        </div>
        <div class="boxs-item">
          <img src="https://rukminim2.flixcart.com/flap/64/64/image/22fddf3c7da4c4f4.png?q=100" alt="">
          <h3>Mobiles</h3>
        </div>
        <div class="boxs-item" id="fashion-item">
          <img src="	https://rukminim2.flixcart.com/fk-p-flap/64/64/image/0d75b34f7d8fbcb3.png?q=100" alt="">
          <h3>Fashion</h3>
          <ul>
            <li>Men's Top Wear</li>
            <li>Men's Bottom Wear</li>
          </ul>
        </div>
        <div class="boxs-item">
          <img src="https://rukminim2.flixcart.com/flap/64/64/image/69c6589653afdb9a.png?q=100" alt="">
          <h3>Electronics</h3>
        </div>
        <div class="boxs-item" id="hf">
          <img src="https://rukminim2.flixcart.com/flap/128/128/image/ab7e2b022a4587dd.jpg?q=100" alt="">
          <h3>Home & Furniture</h3>
        </div>
        <div class="boxs-item">
          <img src="https://rukminim2.flixcart.com/fk-p-flap/64/64/image/0139228b2f7eb413.jpg?q=100" alt="">
          <h3>Appliances</h3>
        </div>
        <div class="boxs-item">
          <img src="https://rukminim2.flixcart.com/flap/64/64/image/71050627a56b4693.png?q=100" alt="">
          <h3>Travel</h3>
        </div>
        <div class="boxs-item" id="toys">
          <img src="https://rukminim2.flixcart.com/flap/128/128/image/dff3f7adcf3a90c6.png?q=100" alt="">
          <h3>Beauty,Toys & More</h3>
        </div>
        <div class="boxs-item" id="wheeler">
          <img src="https://rukminim2.flixcart.com/fk-p-flap/128/128/image/05d708653beff580.png?q=100" alt="">
          <h3>Two Wheelers</h3>
        </div>
      </div>

      
    <!-- slider -->

    <div id="carouselExampleInterval" class="carousel slide" data-bs-ride="carousel">
      <div class="carousel-inner">
        <div class="carousel-item active" data-bs-interval="10000">
          <img src="https://rukminim2.flixcart.com/fk-p-flap/1600/270/image/c49c5f18af00ff00.png?q=20" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item" data-bs-interval="2000">
          <img src="https://rukminim2.flixcart.com/fk-p-flap/1600/270/image/82096d8e3f24023c.jpg?q=20" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item">
          <img src="https://rukminim2.flixcart.com/fk-p-flap/1600/270/image/11cd504bbc3a1493.jpg?q=20" class="d-block w-100" alt="...">
        </div>
      </div>
      <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleInterval" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
      </button>
      <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleInterval" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
      </button>
    </div>
    </div>

 <div id="page3">
  <div class="container" id="electronics-slider">
    <div class="row">
        <h2>Best of Electronics</h2>
      <div class="MultiCarousel" data-items="1,3,5,6" data-slide="1" id="MultiCarousel"  data-interval="1000">
              <div class="MultiCarousel-inner">
                <% gethome.forEach((data)=>{ %>
                  <div class="item">
                      <div class="pad15">
                        <img src="<%= data.image_insert %>" alt="" width="100px" >
                        <h1 class="product-name"><%= data.title %></h1>
                        <p class="product-price"><%= data.price %></p>
                        <form action="/add-cart/<%= data._id%>" method="post">
                            <input type="hidden" name="id" value="<%= data._id %>">
                            <button class="add-to-cart-btn" value="addcart">Add to Cart</button>
                         </form>
                      </div>
                  </div>
                  
              <% }) %>
              </div> 
    <button class="btn btn-primary leftLst"><</button>
    <button class="btn btn-primary rightLst">></button>
  </div>
</div>
</div>
<div id="flight-ticket">
  <img src="/pimage/flight ticket.webp" alt="">
</div>
</div>
  <div class="container2" id="beauty-slider">
    <div class="row" >
        <h2>Beauty,Food,Toys & More</h2>
      <div class="MultiCarousel" data-items="1,3,5,6" data-slide="1" id="MultiCarousel"  data-interval="1000">
              <div class="MultiCarousel-inner">
                <% gethome.forEach((data)=>{ %>
                  
                  <% if (data.categoryname == 'Beauty'){ %>
                    <div class="item">
                      <div class="pad15">
                        <img src="<%= data.image_insert %>" alt="" width="100px" >
                        <h1 class="product-name"><%= data.title %></h1>
                        <p class="product-price"><%= data.price %></p>
                        <form action="/add-cart/<%= data._id%>" method="post">
                            <input type="hidden" name="id" value="<%= data._id %>">
                            <button class="add-to-cart-btn" value="addcart">Add to Cart</button>
                         </form>
                      </div>
                  </div>
                  <% } %>
                  
              <% }) %>
              </div> 
    <button class="btn btn-primary leftLst"><</button>
    <button class="btn btn-primary rightLst">></button>
  </div>
</div>
</div>

<div class="container3" id="featured-slider">
  <div class="row" >
      <h2>Featured Brand</h2>
    <div class="MultiCarousel" data-items="1,2,3,4" data-slide="1" id="MultiCarousel"  data-interval="1000">
            <div class="MultiCarousel-inner">
              <% gethome.forEach((data)=>{ %>
                
                <% if (data.categoryname == 'Featured Brand'){ %>
                  <div class="item">
                    <div class="pad15">
                      <img src="<%= data.image_insert %>" alt="" width="100%" height="100%">
            
                    </div>
                </div>
                <% } %>
                
            <% }) %>
            </div> 
  <button class="btn btn-primary leftLst"><</button>
  <button class="btn btn-primary rightLst">></button>
</div>
</div>
</div>

<div class="container2" id="beauty-slider">
  <div class="row" >
      <h2>Best Fashion Brands For You</h2>
    <div class="MultiCarousel" data-items="1,3,5,6" data-slide="1" id="MultiCarousel"  data-interval="1000">
            <div class="MultiCarousel-inner">
              <% gethome.forEach((data)=>{ %>
                
                <% if (data.categoryname == 'Best fashion Brand'){ %>
                  <div class="item">
                    <div class="pad15">
                      <img src="<%= data.image_insert %>" alt="" width="100px" >
                      <h1 class="product-name"><%= data.title %></h1>
                      <p class="product-price"><%= data.price %></p>
                      <form action="/add-cart/<%= data._id%>" method="post">
                          <input type="hidden" name="id" value="<%= data._id %>">
                          <button class="add-to-cart-btn" value="addcart">Add to Cart</button>
                       </form>
                    </div>
                </div>
                <% } %>
                
            <% }) %>
            </div> 

          
  <button class="btn btn-primary leftLst"><</button>
  <button class="btn btn-primary rightLst">></button>
</div>
</div>
</div>

